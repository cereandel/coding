<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" styleClass="root" stylesheets="@css/MenuPrincipal.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cereandel.Controller">
   <center>
      <VBox alignment="CENTER" prefHeight="285.0" prefWidth="326.0" spacing="40.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="179.0" fitWidth="271.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/logo.png" />
               </image>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#showMenuSecundario" prefHeight="47.0" prefWidth="99.0" text="Jugar">
               <font>
                  <Font name="Tahoma Bold" size="12.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#salir" prefHeight="50.0" prefWidth="99.0" text="Salir" textFill="RED">
               <font>
                  <Font name="Tahoma Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </center>
</BorderPane>
